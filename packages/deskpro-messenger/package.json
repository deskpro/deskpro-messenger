  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "npm-run-all -p watch:*",
    "build": "npm-run-all -s build:*",
    "watch:js": "node scripts/start.js",
    "build:js": "node scripts/build.js",
    "watch:css": "postcss -o public/assets/styles.css --verbose -m -w --env development src/style.css --config ./",
    "build:css": "postcss --verbose --env production -m -o build/assets/styles.css src/style.css --config ./",
    "postbuild": "node ./scripts/build-messenger.js",
    "test": "node scripts/test.js --env=jsdom",
    "storybook": "start-storybook -p 9009 -s public",
    "start-storybook": "npm-run-all -p watch:css storybook",
    "build-storybook": "build-storybook -s public",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "7.1.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@storybook/addon-actions": "^3.4.8",
    "@storybook/addon-knobs": "^3.4.8",
    "@storybook/addon-links": "^3.4.8",
    "@storybook/addons": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "@svgr/webpack": "2.4.1",
    "babel-eslint": "9.0.0",
